<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFD3D3F3"
    tools:context=".EditStoreActivity">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="340dp"
        android:layout_height="469dp"
        android:background="#F4C7E8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.732" />

    <EditText
        android:id="@+id/et_store_name"
        android:layout_width="270dp"
        android:layout_height="55dp"
        android:ems="10"
        android:fontFamily="@font/andada"
        android:inputType="text"
        android:text="Nombre de la Tienda"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.425"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.356" />

    <Button
        android:id="@+id/btn_save_store"
        android:layout_width="226dp"
        android:layout_height="55dp"
        android:backgroundTint="#E882CB"
        android:fontFamily="@font/adlam_display"
        android:text="Guardar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_store_name"
        app:layout_constraintVertical_bias="0.75" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="224dp"
        android:layout_height="34dp"
        android:background="#F4BBE4"
        android:fontFamily="@font/aclonica"
        android:text="Editar Tienda"
        android:textAlignment="center"
        android:textColor="#3A3AED"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/et_store_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.427"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.647" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/aclonica"
        android:text="Nombre:"
        android:textColor="#3A3AEC"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/et_store_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.148"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textLatitud"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:layout_marginBottom="14dp"
        android:fontFamily="@font/aclonica"
        android:text="Latitud:"
        android:textColor="#3A3AEB"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/editLatitud"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.145"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editLatitud"
        android:layout_width="267dp"
        android:layout_height="55dp"
        android:ems="10"
        android:fontFamily="@font/andada"
        android:inputType="text"
        android:text="Latitud de la Tienda"
        app:layout_constraintBottom_toTopOf="@+id/btn_save_store"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.437"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_store_name"
        app:layout_constraintVertical_bias="0.391" />

    <TextView
        android:id="@+id/textLongitud"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:fontFamily="@font/aclonica"
        android:text="Longitud:"
        android:textColor="#3A3AEB"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/editLongitud"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.151"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textLatitud"
        app:layout_constraintVertical_bias="0.658" />

    <EditText
        android:id="@+id/editLongitud"
        android:layout_width="261dp"
        android:layout_height="51dp"
        android:ems="10"
        android:fontFamily="@font/andada"
        android:inputType="text"
        android:text="Longitud de la Tienda"
        app:layout_constraintBottom_toTopOf="@+id/btn_save_store"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.46"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editLatitud"
        app:layout_constraintVertical_bias="0.612" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="286dp"
        android:layout_height="26dp"
        android:background="#FAF7F7"
        android:fontFamily="@font/aclonica"
        android:text="Coordenadas de la Tienda:"
        android:textAlignment="center"
        android:textColor="#3A3AEA"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.448" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="98dp"
        android:layout_height="96dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tienda" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:fontFamily="@font/aclonica"
        android:text="StoreHub"
        android:textColor="@color/purple_500"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.787"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>